PREPARE_TARGETS += prepare.liburl
PREPARE_URL_liburl = wf@allinone.io:alpine/liburl.git
PREPARE_DIR_liburl = $(DEPENDS_LIBRARY_PATH)/$(basename $(notdir $(PREPARE_URL_liburl)))
prepare.liburl:
	-git clone $(PREPARE_URL_liburl) $(PREPARE_DIR_liburl)
	-echo "SUPER_MAKE_DIR=${MAKE_FILE_DIR}"                >> $(PREPARE_DIR_liburl)/.make/super
	-echo "SUPER_MAKE_CONFIG_DIR=${MAKE_CONFIG_DIR}"       >> $(PREPARE_DIR_liburl)/.make/super
	-cd $(PREPARE_DIR_liburl) && make prepare
	-rm $(PREPARE_DIR_liburl)/.make/super
